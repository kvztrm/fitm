-- FINAL EXPLOIT NOTIFIER - WORKS 100%
-- Scans ONLY your listed fish, reads ProximityPrompt.ActionText

local HttpService = game:GetService("HttpService")
local RunService = game:GetService("RunService")

local WEBHOOK_URL = "https://webhook.lewisakura.moe/api/webhooks/1469813322936356956/7vOLfjATJUvnwcKcUACeMOpC6-KssMk6P6pSsNzajaJXHRWY5PhPuq9rE2WJQsNJ2u2n"

local THRESHOLD = 200   -- change to 50 for testing
local COOLDOWN = 10

local fpp = workspace:WaitForChild("FrameWork_WorkSpace"):WaitForChild("FishPPPart")

local folder1 = fpp:WaitForChild("Folder1")
local folder2 = fpp:WaitForChild("Folder2")
local folder3 = fpp:WaitForChild("Folder3")

-- ALL YOUR FISH (exact indices you gave)
local fish = {
