local lastSend = 0

RunService.Heartbeat:Connect(function()
    for _, v in ipairs(fish) do
        local part = v[1]
        local name = v[2]
        if part and part:FindFirstChild("PPPart_PP") then
            local prompt = part.PPPart_PP
            if prompt:IsA("ProximityPrompt") and prompt.ActionText then
                local text = prompt.ActionText
                local percent = text:match("(%d+)%%")
                if percent then
                    local num = tonumber(percent)
                    if num and num >= THRESHOLD then
                        local now = tick()
                        if now - lastSend >= COOLDOWN then
                            lastSend = now
                            HttpService:PostAsync(WEBHOOK_URL, HttpService:JSONEncode({
                                embeds = {{
                                    title = num .. "% FISH",
                                    description = "**" .. name .. "**\n" .. text,
                                    color = 0xFF0000,
                                    timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
                                }}
                            }), Enum.HttpContentType.ApplicationJson)
                            print("[SENT] " .. name .. " - " .. num .. "%")
                        end
                    end
                end
            end
        end
    end
end)

print("FISH NOTIFIER LOADED - WAITING FOR " .. THRESHOLD .. "%+")
